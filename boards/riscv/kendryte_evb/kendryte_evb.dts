/dts-v1/;

#include <riscv64-k210.dtsi>

/ {
	model = "Kendryte Kendryte EVB";
	compatible = "kendryte,kendryte_evb";
	chosen {
		zephyr,console = &uart2;
		zephyr,shell-uart = &uart2;
	};
};

&dma {
	status = "ok";
};

&gpio {
	status = "ok";
};

&gpiohs {
	status = "ok";
};

&i2s0 {
	status = "ok";
};

&spi0 {
	status = "ok";
	cs-gpios = <&gpiohs 7 0>;
	#address-cells = <1>;
	#size-cells = <0>;

	sdhc0: sdhc@0 {
		compatible = "zephyr,mmc-spi-slot";
		reg = <0>;
		status = "ok";
		label = "SDHC0";
		spi-max-frequency = <24000000>;
	};
};

&spi1 {
	status = "ok";
};

&uart0 {
	status = "ok";
	current-speed = <115200>;
	clock-frequency = <16000000>;
};

&uart1 {
	status = "ok";
	current-speed = <115200>;
	clock-frequency = <16000000>;
};

&uart2 {
	status = "ok";
	current-speed = <115200>;
	clock-frequency = <16000000>;
};

&wdt0 {
	status = "ok";
};

&wdt1 {
	status = "ok";
};

